alias hgrep='history|ag'
alias ll='ls -ltrh'
alias la='ls -a'
alias be='bundle exec'
alias duh='du -h --max-depth=1'
alias mv='mv -i'
alias cp='cp -i'

alias dc='docker-compose'
alias dcu='docker-compose up'
alias nobinding='find . -type f -iname '*.rb' -exec sed -i '/binding.pry/d' "{}" +;'

alias open='xdg-open'
